using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Lab1._2
{
    class Program
    {

        public static string abc(string s, int i) {

            return s = s.Remove(i, 1);

        }
        static void Main(string[] args)
        {
            string s;
            int i;
            Console.WriteLine("iveskite stringa: ");
            s = Console.ReadLine();
            Console.WriteLine("iveskite simbolio pozicija: ");
            i = Convert.ToInt32(Console.ReadLine());
            i--;
            Console.WriteLine(abc(s, i));
            Console.ReadLine();
        }
    }
}
