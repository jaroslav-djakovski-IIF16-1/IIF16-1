using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Lab1._2
{
    class Program
    {
        static void Main(string[] args)
        {
            char a;
            Console.Write("Iveskite simboli: ");
            a = Convert.ToChar(Console.ReadLine());
            if (Char.IsDigit(a))
                Console.Write("ivestas simbolis yra skaicius ");
            else if (Char.IsLower(a))
                Console.Write("ivestas simbolis maza raide ");
            else if (Char.IsUpper(a))
                Console.Write("ivestas simbolis didele raide ");
            else
                Console.Write("ivestas simbolis nera raide ");
            Console.ReadLine();
        }
    }
}
