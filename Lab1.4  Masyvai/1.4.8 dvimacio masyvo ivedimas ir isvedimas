using System;
using System.IO;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Lab1
{
	class Program
	{
		static void Main(string[] args)
		{
			int[,] arr = new int [100, 100];
			int x, y;
			string[] var;
			Console.WriteLine("iveskite dvimacio masyvo dydi: ");
			var = Console.ReadLine().Split(' ');
			x = int.Parse(var[0]);
			y = Convert.ToInt32(var[1]);
			//Console.WriteLine(x + "  " + y);

			for (int i = 0; i < x; i++) 
			{
				for (int j = 0; j < y; j++)
				{
					Console.WriteLine("iveskite " + (i + 1) + " " + (j + 1) + " matricos elementa ");
					arr [i, j] = Convert.ToInt32(Console.ReadLine());

				} 

			}

			for (int i = 0; i < x; i++)
			{
				for (int j = 0; j < y; j++)
				{
					Console.Write(arr [i,j] + " ");

				}
				Console.WriteLine();

			}
			Console.ReadLine();

		}
	}
}
